{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Features": {
    "Names": {
      "Uri": "http://uinames.com/api/"
    },
    "Giphy": {
      "Enabled": true,
      "EnabledExpr": "System.DateTime.Now.Hour > 12",
      "UriFormat": "http://api.giphy.com/v1/gifs/search?q={0}&api_key=dc6zaTOxFJmzC&limit=10&offset=0"
    }
  }
}
